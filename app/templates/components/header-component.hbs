         </li>
         {{/if}}

         <li>
           <a href="feedback" class="feedback-tab {{active-route 'feedback' activeRoute}}"> </a>
           <span class="tooltip-text"> Feedback </span>
         </li>

         <li>
           <a class="exit-btn" {{action "logOut"}}> </a>
