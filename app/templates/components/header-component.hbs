<div class="header">
  <div class="row">
    <div class="logo col s2 m2">
      <a href="/"> <img src="/assets/images/icons/png/logo-white.png" alt=""> </a>
    </div>
     <div class="nav col s10 m10">
       <ul class="menu">
         <li>
           <a href="/" class="home-tab {{active-route 'home' activeRoute}}"> </a>
           <span class="tooltip-text"> Inicio </span>
         </li>

         {{#if isAdminUser}}
         <li>
           <a href="/new" class="newmenu-tab {{active-route 'new-lunch' activeRoute}}"> </a>
